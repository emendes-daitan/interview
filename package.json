{
  "name": "feedback",
  "description": "Interview Feedback Generator",
  "authors": [
    "Eduardo Cauli <ecauli@gmail.com> (https://github.com/ecauli"
  ],
  "license": "MIT",
  "version": "0.9.0",
  "main": "bin/server.js",
  "scripts": {
    "start": "concurrently --kill-others \"npm run start-prod\"",
    "start-prod": "bnr start-prod",
    "dev": "concurrently --kill-others \"npm run watch-client\" \"npm run start-dev\"",
    "start-dev": "bnr start-dev",
    "watch-client": "bnr watch-client",
    "build": "bnr build",
    "build-dlls": "webpack --colors --display-error-details --config webpack/vendor.config.js",
    "postinstall": "concurrently \"npm run build\" \"npm run build-dlls\" \"node semantic-fix.js\"",
    "precommit": "lint-staged",
    "prettier": "prettier-eslint --write --eslint-config-path .eslintrc './+(src)/**/*.js'",
    "lint": "eslint -c .eslintrc src --fix",
    "lighthouse-report": "lighthouse http://localhost:8080 --view"
  },
  "lint-staged": {
    "+(src)/**/*.js": [
      "prettier-eslint --write --eslint-config-path .eslintrc",
      "eslint -c .eslintrc",
      "git add"
    ]
  },
  "betterScripts": {
    "start-prod": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production",
        "PORT": 8080,
        "APIPORT": 3030
      }
    },
    "start-dev": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "PORT": 3000,
        "APIPORT": 3030,
        "WEBPACK_DLLS": 1
      }
    },
    "watch-client": {
      "command": "node webpack/webpack-dev-server.js",
      "env": {
        "UV_THREADPOOL_SIZE": 100,
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "PORT": 3000,
        "APIPORT": 3030,
        "WEBPACK_DLLS": 1
      }
    },
    "build": {
      "command": "webpack --colors --display-error-details --config=webpack/prod.config.js",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
